{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.6.4",
      "files": ["jquery.min.js", "jquery.js", "jquery.min.map"],
      "destination": "wwwroot/lib/jquery/dist"
    },
    {
      "library": "jquery-validate@1.19.5",
      "files": ["jquery.validate.js", "jquery.validate.min.js", "additional-methods.js","additional-methods.min.js"],
      "destination": "wwwroot/lib/jquery-validation/dist"
    },
    {
      "library": "jquery-validation-unobtrusive@4.0.0",
      "files": [
        "jquery.validate.unobtrusive.js",
        "jquery.validate.unobtrusive.min.js"
      ],
      "destination": "wwwroot/lib/jquery-validation-unobtrusive"
    },
    {
      "library": "bootstrap@5.3.3",
      "destination": "wwwroot/lib/bootstrap"
    },
    {
      "library": "font-awesome@6.2.1",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/all.min.css",
        "webfonts/*.ttf",
        "webfonts/*.woff2"
      ]
    },
    {
      "library": "datatables.net@2.1.5",
      "destination": "wwwroot/lib/datatables.net",
      "files": [
        "dataTables.min.js",
        "dataTables.js"
      ]
    },
    {
      "library": "datatables.net-bs5@2.1.5",
      "destination": "wwwroot/lib/datatables.net-bs5",
      "files": [
        "dataTables.bootstrap5.min.js",
        "dataTables.bootstrap5.min.css"
      ]
    },
    {
      "library": "datatables.net-select@2.0.5",
      "destination": "wwwroot/lib/datatables.net-select",
      "files": [
        "dataTables.select.min.js"
      ]
    },
    {
      "library": "datatables.net-select-bs5@2.0.5",
      "destination": "wwwroot/lib/datatables.net-select-bs5",
      "files": [
        "select.bootstrap5.min.css"
      ]
    },
    {
      "library": "datatables.net-buttons-bs5@3.1.2",
      "destination": "wwwroot/lib/datatables.net-buttons-bs5",
      "files": [
        "buttons.bootstrap5.min.js",
        "buttons.bootstrap5.min.css"
      ]
    },
    {
      "library": "datatables-buttons@3.1.2",
      "destination": "wwwroot/lib/datatables-buttons",
      "files": [
        "js/dataTables.buttons.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "sweetalert2@11.6.7",
      "destination": "wwwroot/lib/sweetalert2/",
      "files": [
        "dist/sweetalert2.min.js",
        "dist/sweetalert2.min.css"
      ]
    },
    {
      "library": "timepicker@1.3.5",
      "destination": "wwwroot/lib/timepicker",
      "files": [
        "jquery.timepicker.min.js",
        "jquery.timepicker.min.css"
      ]
    },
    {
      "library": "bootstrap-datepicker@1.10.0",
      "destination": "wwwroot/lib/bootstrap-datepicker",
      "files": [
        "js/bootstrap-datepicker.min.js",
        "css/bootstrap-datepicker.min.css"
      ]
    }
  ]
}